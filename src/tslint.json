{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "vl",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "vl",
            "kebab-case"
        ],
        "typedef-whitespace": [false],
        "variable-name": false
    }
}
